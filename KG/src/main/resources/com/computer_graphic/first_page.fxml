<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane maxHeight="910" maxWidth="1280" minHeight="910" minWidth="1280" prefHeight="910.0" prefWidth="1280.0"
            styleClass="first_page" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.computer_graphic.controllers.FirstPageController">

    <children>
        <Group layoutX="560.0" layoutY="180.0"/>
        <VBox alignment="CENTER" layoutX="28.0" layoutY="150.0"/>
        <VBox alignment="CENTER" layoutX="32.0" layoutY="84.0"/>

        <Button fx:id="btnToMenu" layoutX="470.0" layoutY="570.0" mnemonicParsing="false"
                onAction="#switchToMenu" styleClass="start" prefHeight="100.0" prefWidth="400.0"
                text="Давай вчитися">
        </Button>
        <Button fx:id="btnClose" layoutX="470.0" layoutY="700.0" mnemonicParsing="false"
                onAction="#closeProgram" styleClass="startb"
                prefHeight="100.0" prefWidth="400.0" text="Вийти">
        </Button>
    </children>
</AnchorPane>
