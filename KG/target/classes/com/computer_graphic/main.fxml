<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane maxHeight="910" maxWidth="1280" minHeight="910" minWidth="1280" prefHeight="910.0" prefWidth="1280.0"
            styleClass="m" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.computer_graphic.controllers.MainController">

    <children>





        <Button fx:id="btnFractal" layoutX="380.0" layoutY="250.0" mnemonicParsing="false"
                onAction="#switchToFractal"
                prefHeight="120.0" prefWidth="525.0" styleClass="menu" text="Фрактали Ньютона">
        </Button>

        <Button fx:id="btnColors" layoutX="380.0" layoutY="400.0" mnemonicParsing="false"
                onAction="#switchToColors"
                prefHeight="120.0" prefWidth="525.0" styleClass="menu" text="Кольорові моделі">
        </Button>
        <Button fx:id="btnAF" layoutX="380.0" layoutY="550.0" mnemonicParsing="false"
                onAction="#switchToAF"
                prefHeight="120.0" prefWidth="525.0" styleClass="menu" text="Афінні перетворення">
        </Button>

        <Button fx:id="btnExit" layoutX="380.0" layoutY="700.0" mnemonicParsing="false"
                onAction="#exit"
                prefHeight="120.0" prefWidth="525.0" styleClass="menub" text="Вийти">
        </Button>

    </children>
</AnchorPane>
